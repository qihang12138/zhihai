.head-panel {
    display: flex;
    align-items: center;
    padding-left: 30rpx;
    height: 200rpx;
    background-color: #F36886;
    color: #fff;
}

.avatar {
    width: 110rpx;
    height: 110rpx;
    border-radius: 50%;
    overflow: hidden;
}

.head-name {
    padding-left: 30rpx;
}


/* 信息 */

.msg {
    padding: 30rpx 50rpx 0;
    background: #fff;
}

.msg-row {
    display: flex;
    padding-bottom: 20rpx;
}

.msg-item {
    flex: 1;
    padding-left: 40rpx;
    font-size: 14px;
}

.list {
    padding: 38rpx 70rpx;
    font-size: 15px;
    border-bottom: 1rpx solid #f0f0f0;
    position: relative;
}

.id {
    background: url('data:image/png;base64,') no-repeat 0 center #fff;
    background-size: 24rpx 24rpx;
}

.level {
    background: url('data:image/png;base64,') no-repeat 0 center #fff;
    background-size: 24rpx 24rpx;
}

.age {
    background: url('data:image/png;base64,') no-repeat 0 center #fff;
    background-size: 24rpx 24rpx;
}

.tel {
    background: url('data:image/png;base64,') no-repeat 0 center #fff;
    background-size: 22rpx 28rpx;
}

.site {
    background: url('data:image/png;base64,') no-repeat 0 center #fff;
    background-size: 28rpx 28rpx;
}