.head-img {
    background: url('data:image/png;base64,') no-repeat center 50rpx rgb(243, 104, 134);
    background-size: 112rpx 112rpx;
    height: 330rpx;
}

.list {
    padding: 38rpx 70rpx;
    font-size: 15px;
    border-bottom: 1rpx solid #f0f0f0;
    position: relative;
}

.id {
    background: url('data:image/png;base64,') no-repeat 20rpx center #fff;
    background-size: 30rpx 30rpx;
}

.name {
    background: url('data:image/png;base64,') no-repeat 20rpx center #fff;
    background-size: 34rpx 34rpx;
}

.age {
    background: url('data:image/png;base64,') no-repeat 20rpx center #fff;
    background-size: 34rpx 34rpx;
}

.tel {
    background: url('data:image/png;base64,') no-repeat 20rpx center #fff;
    background-size: 29rpx 36rpx;
}

.msg {
    background: url('data:image/png;base64,') no-repeat 20rpx center #fff;
    background-size: 34rpx 34rpx;
}

.service {
    background: url('data:image/png;base64,') no-repeat 20rpx center #fff;
    background-size: 34rpx 31rpx;
}

.cooperation {
    background: url('data:image/png;base64,') no-repeat 20rpx center #fff;
    background-size: 34rpx 30rpx;
}

.list::after {
    content: '';
    position: absolute;
    top: 45rpx;
    right: 20rpx;
    width: 15rpx;
    height: 15rpx;
    border-top: 4rpx solid #ddd;
    border-right: 4rpx solid #ddd;
    transform: rotate(45deg);
}