<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{circular}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <navigator url="">
        <image src="{{item}}" class="slide-image" />
      </navigator>
    </swiper-item>
  </block>
</swiper>
<!-- 舞种分类 -->
<view class="dance">
  <block wx:for="{{8}}">
    <navigator class="dance-list" url="">
      <image src="https://s2.ax1x.com/2019/07/15/ZoFuee.png" class="dance-image"></image>
      <view>拉丁舞</view>
    </navigator>
  </block>
</view>
<!-- 选项卡 -->
<view class="tabs">
  <view class="tabs-list">
    <text class="tabs-on">综合</text>
    <text>附近</text>
    <text>舞种</text>
  </view>
  <view class="tabs-images">
    <block wx:for="{{10}}">
      <navigator url="">
        <image src="https://s2.ax1x.com/2019/07/15/ZoFuee.png" class="tabs-image"></image>
      </navigator>
    </block>
  </view>
</view>
<!-- 新闻 -->
<view class="news">
  <view class="news-tit"></view>
  <block wx:for="{{4}}">
    <navigator class="" url="">
      <view class="news-cont">
        <image src="https://s2.ax1x.com/2019/07/15/ZoFuee.png" class="news-image"></image>
        <view class="news-deta">
          <view class="news-head">分校介绍及计划</view>
          <view class="news-text">点点星舞蹈现有七所校区，分别为锦艺城总校区大学路校区、须水校区、郑东经开校区</view>
          <text class="news-time">2019-7-15</text>
        </view>
      </view>
    </navigator>
  </block>
</view>