page {
    background: #f4f5f6;
}

.slide-image {
    width: 100%;
    height: 320rpx;
}

.partners {
    padding: 20rpx 0 30rpx;
    background: #fff;
}

.partners-tit {
    padding-bottom: 10rpx;
    text-align: center;
    border-bottom: 1rpx solid #f0f0f0;
}

.partners-titimg {
    height: 40rpx;
    width: 200rpx;
}

.partners-img {
    width: 160rpx;
    height: 160rpx;
    /* border: 1rpx solid #f2f2f2; */
    margin: 9rpx;
}

.partners-cont {
    display: flex;
    flex-wrap: wrap;
    padding: 20rpx 19rpx;
    font-size: 0;
}


/* 选项卡 */

.tabs {
    margin-bottom: 40rpx;
}

.tabs-list {
    display: flex;
    justify-content: space-between;
    margin-top: 20rpx;
    padding: 30rpx 20rpx;
    border-bottom: 1rpx solid #f0f0f0;
    background: #fff;
}

.tabs-tit {
    display: flex;
    padding: 30rpx;
}

.tabs-img {
    height: 31rpx;
    width: 126rpx;
}

.tabs-more {
    flex: 1;
    text-align: right;
    font-size: 12px;
    color: #999
}

.tabs-list text {
    margin-right: 20rpx;
    font-size: 14px;
    color: #222;
}

.tabs-list .tabs-on {
    color: #f36886;
    font-size: 16px;
}


/* 机构列表 */

.organ {
    margin-top: 10rpx;
}

.organ-brief {
    display: flex;
    position: relative;
    padding: 15rpx;
    margin-bottom: 20rpx;
    box-shadow: 0px 4px 6px 0px rgba(105, 105, 105, 0.2);
    background: #fff;
}

.organ-item {
    flex: 1;
    max-width: 500rpx;
}

.organ-tit {
    font-size: 14px;
    color: #333;
}

.organ-msg {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding-top: 10rpx;
    font-size: 12px;
    color: #666;
}

.organ-img {
    margin-right: 23rpx;
    width: 130rpx;
    height: 130rpx;
    border: 1rpx solid #f0f0f0;
}

.check {
    width: 164rpx;
    height: 56rpx;
    line-height: 56rpx;
    font-size: 13px;
    text-align: center;
    color: #F36886;
    border: 1px solid #F36886;
    border-radius: 10rpx;
    background: #fff;
    position: absolute;
    right: 20rpx;
    top: 100rpx;
}

.nav {
    width: 60rpx;
    background: url('data:image/png;base64,') no-repeat right bottom/30rpx 30rpx;
}

.label {
    display: inline-block;
    text-align: center;
    border-radius: 10rpx;
    margin-left: 20rpx;
    padding: 10rpx 15rpx;
    font-size: 12px;
    color: #999;
    background: rgb(244, 245, 246);
}


/* 机构大学 */

.news {
    background: #fff;
}

.news-cont {
    display: flex;
    padding: 20rpx;
    border-bottom: 1rpx solid #f0f0f0;
}

.news-head {
    font-size: 13px;
}

.news-deta {
    flex: 1;
    position: relative;
    font-size: 12px;
}

.news-image {
    height: 140rpx;
    width: 150rpx;
    margin-right: 27rpx;
}

.news-time {
    position: absolute;
    right: 15rpx;
    top: 90rpx;
    color: #97979F;
    font-size: 12px;
}