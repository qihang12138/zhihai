.slide-image {
    width: 100%;
    height: 320rpx;
}

.partners {
    margin: 20rpx 0;
    padding-bottom: 30rpx;
}

.partners-tit {
    height: 90rpx;
    border-bottom: 1rpx solid #f0f0f0;
    background: url('data:image/png;base64,') no-repeat center center;
    background-size: 200rpx 40rpx;
}

.partners-img,
.tabs-image {
    width: 230rpx;
    height: 128rpx;
    border: 1rpx solid #f2f2f2;
    margin: 10rpx 5rpx
}

.partners-cont,
.tabs-images {
    display: flex;
    flex-wrap: wrap;
    padding: 20rpx 13rpx;
    font-size: 0;
}


/* 选项卡 */

.tabs-list {
    border-bottom: 1rpx solid #f0f0f0;
    padding: 30rpx 20rpx;
}

.tabs-list text {
    margin-right: 20rpx;
    font-size: 14px;
    color: #222;
}

.tabs-list .tabs-on {
    color: #F36886;
    font-size: 16px;
}