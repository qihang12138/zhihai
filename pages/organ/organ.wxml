<navigator url="{{pageData.banner.url}}">
  <image src="{{pageData.banner.image}}" class="slide-image" />
</navigator>
<!-- 合作机构 -->
<view class="partners">
  <view class="partners-tit"></view>
  <view class="partners-cont">
    <navigator url="/pages/company/company?id={{item.id}}" wx:for="{{pageData.cooper}}" wx:key="index">
      <image src="{{item.logo}}" class="partners-img" mode="aspectFill" />
    </navigator>
  </view>
</view>
<!-- 选项卡 -->
<view class="tabs">
  <view class="tabs-list">
    <text class="{{tabsOn==0?'tabs-on':''}}" bindtap="tabsOn" data-id="0">综合</text>
    <text class="{{tabsOn==1?'tabs-on':''}}" bindtap="tabsOn" data-id="1">附近</text>
    <text class="{{tabsOn==2?'tabs-on':''}}" bindtap="tabsOn" data-id="2">优秀</text>
  </view>
  <view class="tabs-images">
    <block wx:for="{{tabs}}" wx:key="index">
      <navigator url="/pages/company/company?id={{item.id}}">
        <image src="{{item.logo}}" class="tabs-image" mode="aspectFill"></image>
      </navigator>
    </block>
  </view>
</view>