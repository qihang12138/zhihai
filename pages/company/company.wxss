page {
    background: #F4F5F6;
    padding-bottom: 150rpx;
}

.redBar {
    background: #f36886;
    height: 83rpx;
}

.van-tab--active {
    color: #fff !important;
    background-color: #F36886 !important;
}


/* 公司信息名片 */

.visitingCard {
    background: #fff;
    margin: -70rpx 20rpx 20rpx;
    padding: 40rpx 45rpx 10rpx 45rpx;
    border-radius: 10rpx;
}

.visitingCard-tit {
    font-size: 18px;
    color: #222;
    font-weight: 500;
    display: flex;
    margin-bottom: 20rpx;
}

.visitingCard-name {
    flex: 1;
}

.view {
    font-size: 12px;
    color: #999;
    text-align: right;
    width: 70rpx;
    height: 22rpx;
    line-height: 22rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 30rpx 22rpx;
}

.brief {
    display: flex;
    align-items: center;
}

.brief-text {
    font-size: 14px;
    margin-right: 15rpx;
}

.darkSpot {
    width: 8rpx;
    height: 8rpx;
    background: #333;
    border-radius: 50%;
}

.visitingCard-cont {
    width: 475rpx;
    font-size: 13px;
    color: #666;
    padding: 23rpx 0 30rpx 0;
}

.address {
    display: flex;
    padding-top: 30rpx;
}

.address-text {
    font-size: 14px;
    padding: 0 50rpx;
    flex: 1;
    background: url('data:image/png;base64,') no-repeat 0 center/17rpx 23rpx;
}

.nav {
    width: 70rpx;
    background: url('data:image/png;base64,') no-repeat 0 center/30rpx 30rpx;
}

.tel {
    margin-top: 25rpx;
    padding: 15rpx 50rpx;
    font-size: 14px;
    color: #666;
    border-top: 1px solid #f0f0f0;
    background: url('data:image/png;base64,') no-repeat 0 center/20rpx 20rpx;
}


/* 选项卡 */

.tabs {
    padding-top: 100rpx!important;
}


/* 公司简介 */

.aboutUs {
    background: #fff;
    padding: 0 60rpx 120rpx;
}

.aboutUs-tit {
    font-size: 16px;
    color: #222;
    padding: 30rpx 0;
}

.aboutUs-img {
    width: 630rpx;
    height: 322rpx;
    margin-bottom: 10rpx;
}


/* 招聘 */

.careers {
    display: flex;
    margin: 0 20rpx 20rpx 20rpx;
    padding: 40rpx;
    border-radius: 10rpx;
    background: #fff;
    box-shadow: 0 3px 10px 0 rgba(234, 234, 234, 1);
}

.careers-img {
    margin-right: 30rpx;
    height: 94rpx;
    width: 94rpx;
}

.careers-text text {
    margin-right: 10rpx;
    font-size: 14px;
    color: #5f5f6b;
}

.careers-pay {
    color: #D86381;
}