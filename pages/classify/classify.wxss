/* 下拉框 */
.classify {
    display: flex;
    width: 750rpx;
}

.collapse {
    display: flex;
    width: 750rpx;
}

.collapse-item {
    flex: 1 !important;
}

/* 舞者 */
.dancer {
    padding-bottom: 30rpx;
    border-bottom: 1rpx solid #f0f0f0;
    background: #fff;
}

.dancer-brief {
    padding: 30rpx 20rpx;
    display: flex;
    position: relative;
}

.dancer-msg {
    font-size: 12px;
    color: #999;
}

.dancer-img {
    width: 94rpx;
    height: 94rpx;
    border-radius: 10rpx;
    margin-right: 30rpx;
}

.check {
    width: 164rpx;
    height: 56rpx;
    line-height: 56rpx;
    font-size: 13px;
    color: #F36886;
    border: 1px solid #F36886;
    border-radius: 10rpx;
    background: #fff;
    position: absolute;
    right: 20rpx;
    top: 100rpx;
}

.label {
    display: inline-block;
    text-align: center;
    border-radius: 10rpx;
    margin-left: 20rpx;
    padding: 10rpx 15rpx;
    font-size: 12px;
    color: #999;
    background: rgb(244, 245, 246);
}