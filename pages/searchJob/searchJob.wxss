.nodata {
    margin-top: 300rpx;
    text-align: center;
    color: #999;
}
/* 舞者 */
.dancer {
    padding-bottom: 30rpx;
    border-bottom: 1rpx solid #f0f0f0;
    background: #fff;
}

.dancer-brief {
    padding: 30rpx 20rpx;
    display: flex;
    position: relative;
}

.dancer-msg {
    font-size: 12px;
    color: #999;
}

.dancer-img {
    width: 94rpx;
    height: 94rpx;
    border-radius: 10rpx;
    margin-right: 30rpx;
}
.check {
    width: 164rpx;
    height: 56rpx;
    line-height: 56rpx;
    text-align: center;
    font-size: 13px;
    color: #F36886;
    border: 1px solid #F36886;
    border-radius: 10rpx;
    background: #fff;
    position: absolute;
    right: 20rpx;
    top: 100rpx;
}

.label {
    display: inline-block;
    text-align: center;
    border-radius: 10rpx;
    margin-left: 20rpx;
    padding: 10rpx 15rpx;
    font-size: 12px;
    color: #999;
    background: rgb(244, 245, 246);
}

.suggest {
    padding-left: 20rpx;
    font-size: 12px;
    color: #999;
}

.suggest-nav {
    display: inline-block;
}

.option {
    border: 1rpx solid rgba(204, 204, 204, 1);
    border-radius: 50rpx;
    padding: 11rpx 30rpx;
    font-size: 12px;
    color: rgba(153, 153, 153, 1);
    margin: 30rpx 0 0 20rpx;
    display: inline-block;
}