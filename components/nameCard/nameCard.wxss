page {
    background: #f4f5f6;
}

.redBar {
    background: #f36886;
    height: 190rpx;
}


/* 个人信息名片 */

.nameCard {
    margin: -120rpx 20rpx 0;
    padding: 40rpx 45rpx;
    border-radius: 10rpx;
    text-align: center;
    color: #222;
    background: #fff;
}

.nameCard-img {
    margin: -100rpx auto 20rpx;
    height: 130rpx;
    width: 130rpx;
    border-radius: 50%;
    background: url('data:image/png;base64,') no-repeat 0 0 /130rpx 130rpx;
}

.nameCard-name {
    font-size: 18px;
}

.nameCard-job {
    font-size: 14px;
    margin: 13rpx 0 28rpx;
}

.nameCard-brief {
    font-size: 13px;
    width: 464rpx;
    margin: 0 auto;
}

.nameCard-site {
    display: inline-block;
    margin: 35rpx;
    padding-left: 30rpx;
    font-size: 14px;
    background: url('data:image/png;base64,') no-repeat 0 center/17rpx 23rpx
}


/* 联系按钮 */

.contact {
    position: fixed;
    bottom: 0;
    left: 0;
    padding: 20rpx;
    background: #fff;
}

.contact-btn {
    width: 710rpx;
    height: 70rpx;
    font-size: 14px;
    background: #f36886;
    color: #fff;
}